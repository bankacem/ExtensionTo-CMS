{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://backend.extensionto.workers.dev/api/:path*"
    },
    {
      "source": "/sitemap.xml",
      "destination": "https://backend.extensionto.workers.dev/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "https://backend.extensionto.workers.dev/robots.txt"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
